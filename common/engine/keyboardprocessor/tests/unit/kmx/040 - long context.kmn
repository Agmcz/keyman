c Description: Tests context longer than 64 chars
c keys: [K_SPACE][K_F]
c expected: fffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff\u0020
c context:  fffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff

store(&version) '6.0'

begin Unicode > use(Main)

group(Main) using keys

+ [K_SPACE] > U+0020

c prevent entering symbols after space
U+0020 + 'f' > U+0020
